<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="us.mn.state.health.lims.requester.valueholder.RequesterType" table="requester_type" optimistic-lock="version" dynamic-update="true">
    <id name="id" type="us.mn.state.health.lims.hibernate.resources.usertype.LIMSStringNumberUserType">
            <column name="ID" precision="10" scale="0" />
            <generator class="us.mn.state.health.lims.hibernate.resources.StringSequenceGenerator">
            	<param name="sequence_name">requester_type_seq</param>
            </generator>
        </id>
    <property name="requesterType" column="requester_type"></property>
    </class>
</hibernate-mapping>
